{
  "expo": {
    "name": "charto",
    "slug": "charto",
    "owner": "dev-aditya-lab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "charto",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "googleClientId": "964628505731-gm8crktcbr9fkoaetcv4t4lfjpil0f62.apps.googleusercontent.com"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devaditya.charto",
      "associatedDomains": ["applinks:auth.expo.io"]
    },
    "android": {
      "package": "com.devaditya.charto",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "auth.expo.io"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
